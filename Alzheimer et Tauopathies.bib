	year = {2018},
	pmid = {30314521},
	pmcid = {PMC6186090},
	keywords = {Alzheimer Disease, Hippocampus, Neurons, Alzheimer’s disease, tau Proteins, Amyloid beta-Peptides, Animals, Mice, Transgenic, Disease Models, Animal, MicroRNAs, In situ hybridization, microRNA, miR-mimic, miRNA-seq, Real-Time Polymerase Chain Reaction, Up-Regulation},
